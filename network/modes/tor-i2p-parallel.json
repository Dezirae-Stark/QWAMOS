{
  "mode": "tor-i2p-parallel",
  "description": "Tor + I2P parallel (access both networks)",
  "tor_enabled": true,
  "i2p_enabled": true,
  "dnscrypt_enabled": true,
  "vpn_enabled": false,

  "tor": {
    "socks_port": 9050,
    "control_port": 9051,
    "use_bridges": false,
    "circuit_build_timeout": 60,
    "circuit_isolation": true
  },

  "i2p": {
    "http_proxy": "127.0.0.1:4444",
    "socks_proxy": "127.0.0.1:4447",
    "tunnel_length": 3,
    "inbound_tunnels": 3,
    "outbound_tunnels": 3
  },

  "dnscrypt": {
    "listen_address": "127.0.0.1:5353",
    "server_names": ["cloudflare", "quad9"],
    "require_dnssec": true,
    "require_nolog": true
  },

  "routing": {
    "clearnet_via": "tor",
    "i2p_sites_via": "i2p",
    "dns_via": "dnscrypt"
  },

  "firewall": {
    "block_non_tor": true,
    "block_ipv6": true,
    "allow_lan": false,
    "killswitch_enabled": true
  },

  "performance": {
    "expected_speed": "medium",
    "latency": "medium-high",
    "anonymity_level": "very-high"
  }
}
